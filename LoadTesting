import csv

def generate_sql_insert(csv_file):
    sql_insert_statements = []

    with open(csv_file, 'r') as csvfile:
        reader = csv.DictReader(csvfile)
        for row in reader:
            nam = row['NAM']
            des = row['DES']
            parent_id = row['PARENT ID']

            sql_insert = f"INSERT INTO AC_TEC_CAP_PARNT_ID (NAM, DES, PARENT_ID) VALUES ('{nam}', '{des}', '{parent_id}');"
            sql_insert_statements.append(sql_insert)

    return sql_insert_statements

def main():
    csv_file_path = 'your_input_csv_file.csv'  # Update with the path to your CSV file
    sql_insert_statements = generate_sql_insert(csv_file_path)

    with open('output_sql_inserts.sql', 'w') as sql_file:
        sql_file.write('\n'.join(sql_insert_statements))

if __name__ == "__main__":
    main()
